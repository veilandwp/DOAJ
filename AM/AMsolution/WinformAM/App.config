    <add key="connection" value="Data Source=.\sqlexpress;Initial Catalog=DOAJ;Integrated Security=True" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="username" value="admin"/>
    <add key="password" value="123456"/>
